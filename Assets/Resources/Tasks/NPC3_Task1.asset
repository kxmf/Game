    \u0440\u0430\u0431\u043E\u0442\u0430\u043B\u0438 \u0441\u043E \u0441\u0442\u0440\u043E\u043A\u043E\u0432\u044B\u043C\u0438
    \u0434\u0430\u043D\u043D\u044B\u043C\u0438. \u042D\u0442\u043E \u0441\u0432\u044F\u0437\u0430\u043D\u043E
    \u0441 \u0442\u0435\u043C, \u0447\u0442\u043E \u0444\u0443\u043D\u043A\u0446\u0438\u044F
    input() \u0432\u0441\u0435\u0433\u0434\u0430 \u0441\u0447\u0438\u0442\u044B\u0432\u0430\u0435\u0442
    \u0432\u0432\u043E\u0434 \u043A\u0430\u043A \u0441\u0442\u0440\u043E\u043A\u0443.
    \u041E\u0434\u043D\u0430\u043A\u043E \u0447\u0430\u0441\u0442\u043E \u0432\u043E\u0437\u043D\u0438\u043A\u0430\u0435\u0442
    \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E\u0441\u0442\u044C
    \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C \u0441 \u0447\u0438\u0441\u043B\u0430\u043C\u0438.
    \u0427\u0442\u043E\u0431\u044B \u0441\u043E\u0437\u0434\u0430\u0442\u044C \u043F\u0435\u0440\u0435\u043C\u0435\u043D\u043D\u0443\u044E
    \u0446\u0435\u043B\u043E\u0447\u0438\u0441\u043B\u0435\u043D\u043D\u043E\u0433\u043E
    \u0442\u0438\u043F\u0430 \u0432 Python, \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E
    \u0438\u0437\u0431\u0430\u0432\u0438\u0442\u044C\u0441\u044F \u043E\u0442 \u043A\u0430\u0432\u044B\u0447\u0435\u043A
    \u043F\u0440\u0438 \u043F\u0440\u0438\u0441\u0432\u0430\u0438\u0432\u0430\u043D\u0438\u0438
    \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u044F. \u0420\u0430\u0441\u0441\u043C\u043E\u0442\u0440\u0438\u043C
    \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0438\u0439 \u043F\u0440\u0438\u043C\u0435\u0440
    \u043A\u043E\u0434\u0430:\r\n\r\nnum = 15  # num - \u044D\u0442\u043E \u0447\u0438\u0441\u043B\u043E\r\nprint(num)\r\n\r\n\r\n\u0412
    \u0440\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u0435 \u0432\u044B\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u044F
    \u0442\u0430\u043A\u043E\u0439 \u043F\u0440\u043E\u0433\u0440\u0430\u043C\u043C\u044B
    \u0431\u0443\u0434\u0435\u0442 \u0432\u044B\u0432\u0435\u0434\u0435\u043D\u043E
    \u0447\u0438\u0441\u043B\u043E 15.\r\n\r\n\u0411\u0430\u0437\u043E\u0432\u044B\u0435
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438 \u0441 \u0447\u0438\u0441\u043B\u0430\u043C\u0438\r\n\u0412
    \u044F\u0437\u044B\u043A\u0435 Python, \u043A\u0430\u043A \u0438 \u0432 \u043C\u0430\u0442\u0435\u043C\u0430\u0442\u0438\u043A\u0435,
    \u043D\u0430\u0434 \u0447\u0438\u0441\u043B\u0430\u043C\u0438 \u043C\u043E\u0436\u043D\u043E
    \u0441\u043E\u0432\u0435\u0440\u0448\u0430\u0442\u044C 4 \u043E\u0441\u043D\u043E\u0432\u043D\u044B\u0435
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438 (+, -, *, /):\r\n\u201C+\u201D
    \u2013 \u0441\u043B\u043E\u0436\u0435\u043D\u0438\u0435\r\n\u201C-\u201D \u2013
    \u0432\u044B\u0447\u0438\u0442\u0430\u043D\u0438\u0435\r\n\u201C*\u201D \u2013
    \u0443\u043C\u043D\u043E\u0436\u0435\u043D\u0438\u0435\r\n\u201C/\u201D - \u0434\u0435\u043B\u0435\u043D\u0438\u0435\r\n\r\na
    = 3\r\nb = 2\r\n\r\nprint(a + b)\r\nprint(a - b)\r\nprint(a * b)\r\nprint(a /
    b)\r\n\r\n\r\n\r\n\u041F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D\u0438\u0435
    \u0442\u0438\u043F\u043E\u0432\r\n\u0414\u043B\u044F \u0442\u043E\u0433\u043E
    \u0447\u0442\u043E\u0431\u044B \u043F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u0442\u044C
    \u0441\u0442\u0440\u043E\u043A\u0443 \u043A \u0446\u0435\u043B\u043E\u043C\u0443
    \u0447\u0438\u0441\u043B\u0443, \u043C\u044B \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u0435\u043C
    \u043A\u043E\u043C\u0430\u043D\u0434\u0443 int().\r\n\r\n\r\nstr = '1900'\r\namount
    = int(s)\r\nprint(amount)\r\n\r\n\r\n\u041F\u0435\u0440\u0435\u043C\u0435\u043D\u043D\u0430\u044F
    str \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u044F\u0435\u0442
    \u0441\u043E\u0431\u043E\u0439 \u0441\u0442\u0440\u043E\u043A\u0443. \u0418\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u044F
    \u0444\u0443\u043D\u043A\u0446\u0438\u044E int(), \u043C\u044B \u043F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043B\u0438
    \u0441\u0442\u0440\u043E\u043A\u043E\u0432\u043E\u0435 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435
    \u0432 \u0447\u0438\u0441\u043B\u043E. \r\n\r\n\r\n\u0424\u0443\u043D\u043A\u0446\u0438\u044F
    int() \u043F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u0443\u0435\u0442 \u043F\u0435\u0440\u0435\u0434\u0430\u043D\u043D\u043E\u0435
    \u0435\u0439 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435 \u0432 \u0446\u0435\u043B\u043E\u0435
    \u0447\u0438\u0441\u043B\u043E. \u0412 \u0438\u0442\u043E\u0433\u0435 \u043F\u0435\u0440\u0435\u043C\u0435\u043D\u043D\u0430\u044F
