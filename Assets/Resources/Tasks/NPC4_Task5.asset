    \u0432 5-\u0431\u0430\u043B\u043B\u044C\u043D\u0443\u044E:\r\n\r\ngrade = int(input('\u0412\u0432\u0435\u0434\u0438\u0442\u0435
    \u0432\u0430\u0448\u0443 \u043E\u0442\u043C\u0435\u0442\u043A\u0443 \u043F\u043E
    100-\u0431\u0430\u043B\u043B\u044C\u043D\u043E\u0439 \u0441\u0438\u0441\u0442\u0435\u043C\u0435:
    '))\r\n\r\nif grade >= 90:\r\n    print(5)\r\nelse:\r\n    if grade >= 80:\r\n       
    print(4)\r\n    else:\r\n        if grade >= 70:\r\n            print(3)\r\n       
    else:\r\n            if grade >= 60:\r\n                print(2)\r\n           
    else:\r\n                print(1)\r\n\r\n\r\n\u0417\u0434\u0435\u0441\u044C \u043A\u0430\u0436\u0434\u043E\u0435
    \u043D\u043E\u0432\u043E\u0435 \u0443\u0441\u043B\u043E\u0432\u0438\u0435 \u0432\u043B\u043E\u0436\u0435\u043D\u043E
    \u0432\u043D\u0443\u0442\u0440\u044C \u043F\u0440\u0435\u0434\u044B\u0434\u0443\u0449\u0435\u0433\u043E,
    \u0438 \u043E\u0442\u0441\u0442\u0443\u043F\u044B \u0441\u0442\u0430\u043D\u043E\u0432\u044F\u0442\u0441\u044F
    \u0432\u0441\u0451 \u0433\u043B\u0443\u0431\u0436\u0435.\r\n\r\n\u041B\u0443\u0447\u0448\u0435
    \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C \u043A\u0430\u0441\u043A\u0430\u0434\u043D\u044B\u0439
    \u0443\u0441\u043B\u043E\u0432\u043D\u044B\u0439 \u043E\u043F\u0435\u0440\u0430\u0442\u043E\u0440
